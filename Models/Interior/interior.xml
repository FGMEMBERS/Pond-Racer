<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>interior.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>interieur</object-name>
    <object-name>inttour</object-name>
    <object-name>planchet</object-name>

    <!-- Objets transparents -->
    <object-name>intvitres</object-name>
  </animation>

  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>intvitres</object-name>
  </effect>
  
  <!-- Tableau de bord -->
  <model>
    <path>Aircraft/Pond-Racer/Models/Interior/Panel/panel.xml</path>
    <offsets>
      <x-m> -0.524 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.049 </z-m>
    </offsets>
  </model>

  <!-- Pilote -->
  <model>
    <path>Aircraft/Pond-Racer/Models/Pilot/pilot.xml</path>
    <offsets>
      <x-m> 0.326 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.039 </z-m>
    </offsets>
  </model>

  <!-- Seat -->
  <model>
    <path>Aircraft/Pond-Racer/Models/Interior/Seat/seat.xml</path>
    <offsets>
      <x-m> 0.590 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.010 </z-m>
    </offsets>
  </model>
  
</PropertyList>
