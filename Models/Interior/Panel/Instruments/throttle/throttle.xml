    <type>pick</type>
    <visible>true</visible>
    <object-name>mixturemanetteL</object-name>
    <action>
      <button>3</button>
      <binding>
        <command>property-adjust</command>
        <property>controls/engines/engine[0]/mixture</property>
        <step>0.1</step>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>mixturemanetteL</object-name>
    <action>
      <button>4</button>
      <binding>
        <command>property-adjust</command>
        <property>controls/engines/engine[0]/mixture</property>
        <step>-0.1</step>
      </binding>
    </action>
  </animation>

  <!-- Mixture Droite -->
