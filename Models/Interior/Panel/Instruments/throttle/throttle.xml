<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>throttle.ac</path>

  <animation>
    <object-name>throttle</object-name>
    <object-name>cache</object-name>
    <object-name>throttlemanetteL</object-name>
    <object-name>throttlemanetteR</object-name>
    <object-name>mixturemanetteL</object-name>
    <object-name>mixturemanetteR</object-name>
    <object-name>propellermanetteL</object-name>
    <object-name>propellermanetteR</object-name>
  </animation>

  <!-- Manette des gazs -->
  <animation>
    <type>rotate</type>
    <object-name>throttlemanetteL</object-name>
    <property>controls/engines/engine[0]/throttle</property>
    <factor> -65 </factor>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>throttlemanetteR</object-name>
    <property>controls/engines/engine[1]/throttle</property>
    <factor> -65 </factor>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
 
  <!-- Manette de mixture -->
  <animation>
    <type>rotate</type>
    <object-name>mixturemanetteL</object-name>
    <property>controls/engines/engine[0]/mixture</property>
    <factor> -65 </factor>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>mixturemanetteR</object-name>
    <property>controls/engines/engine[1]/mixture</property>
    <factor> -65 </factor>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
 
  <!-- Manette de pas d'hélice -->
  <animation>
    <type>rotate</type>
    <object-name>propellermanetteL</object-name>
    <property>controls/engines/engine[0]/propeller-pitch</property>
    <factor> -70 </factor>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>propellermanetteR</object-name>
    <property>controls/engines/engine[1]/propeller-pitch</property>
    <factor> -70 </factor>
    <offsets>
      <x-m>  0.00 </x-m>
      <y-m>  0.00 </y-m>
      <z-m> -0.03 </z-m>
    </offsets>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <!-- Ajustage par molette -->
  <!-- Puissance Gauche -->
  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>throttlemanetteL</object-name>
    <action>
      <button>3</button>
      <binding>
        <command>property-adjust</command>
        <property>controls/engines/engine[0]/throttle</property>
        <step>0.1</step>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>throttlemanetteL</object-name>
    <action>
      <button>4</button>
      <binding>
        <command>property-adjust</command>
        <property>controls/engines/engine[0]/throttle</property>
        <step>-0.1</step>
      </binding>
    </action>
  </animation>

  <!-- Puissance Droite -->
  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>throttlemanetteR</object-name>
    <action>
      <button>3</button>
      <binding>
        <command>property-adjust</command>
        <property>controls/engines/engine[1]/throttle</property>
        <step>0.1</step>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>throttlemanetteR</object-name>
    <action>
      <button>4</button>
      <binding>
        <command>property-adjust</command>
        <property>controls/engines/engine[1]/throttle</property>
        <step>-0.1</step>
      </binding>
    </action>
  </animation>

  <!-- Mixture Gauche -->
