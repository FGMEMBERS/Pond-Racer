    <type>pick</type>
    <visible>true</visible>
    <object-name>mixturemanetteR</object-name>
    <action>
      <button>3</button>
      <binding>
        <command>property-adjust</command>
        <property>controls/engines/engine[1]/mixture</property>
        <step>0.1</step>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>mixturemanetteR</object-name>
    <action>
      <button>4</button>
      <binding>
        <command>property-adjust</command>
        <property>controls/engines/engine[1]/mixture</property>
        <step>-0.1</step>
      </binding>
    </action>
  </animation>

  <!-- Hélice Gauche -->
  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>propellermanetteL</object-name>
    <action>
      <button>3</button>
      <binding>
        <command>property-adjust</command>
        <property>controls/engines/engine[0]/propeller-pitch</property>
        <step>0.1</step>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>propellermanetteL</object-name>
    <action>
      <button>4</button>
      <binding>
        <command>property-adjust</command>
        <property>controls/engines/engine[0]/propeller-pitch</property>
        <step>-0.1</step>
      </binding>
    </action>
  </animation>

  <!-- Hélice Droite -->
  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>propellermanetteR</object-name>
    <action>
      <button>3</button>
      <binding>
        <command>property-adjust</command>
        <property>controls/engines/engine[1]/propeller-pitch</property>
        <step>0.1</step>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>propellermanetteR</object-name>
    <action>
      <button>4</button>
      <binding>
        <command>property-adjust</command>
        <property>controls/engines/engine[1]/propeller-pitch</property>
        <step>-0.1</step>
      </binding>
    </action>
  </animation>

</PropertyList>
